COPY (select auth_user.id as user_id, email, wine_id, overall from personality_wineratingdata join auth_user on auth_user.id = personality_wineratingdata.user_id join accounts_userprofile on accounts_userprofile.user_id = personality_wineratingdata.user_id join accounts_address on accounts_userprofile.shipping_address_id = accounts_address.id where accounts_address.state = 'CA' order by user_id, wine_id) TO '/tmp/ca-ratings.csv' with csv header;