COPY (select u.id as user_id, email, wine_id, overall from auth_user u join accounts_userprofile profile on u.id = profile.user_id join personality_wineratingdata ratings on ratings.user_id = u.id where u.id in (select invitee_id from main_partyinvite invite join main_party party on party.id = invite.party_id join accounts_address addr on addr.id = party.address_id where state = 'CA') order by user_id, wine_id) TO '/tmp/ca-ratings.csv' with csv header;